{
    "version": "2.0.0",
    "tasks": [

        {
            "command": "rm -rf ${cwd}/discord.ipynb/build/*",
            "type": "shell",
            "label": "clear_build"
        },

        {
            "command": "tsc --project ${cwd}/discord.ipynb",
            "type": "shell",
            "label": "compile_ts"
        },

        {
            "command": "node ${cwd}/discord.ipynb/build/index.js",
            "label": "launch",
            "type": "shell",
            "dependsOn": [ "clear_build", "compile_ts" ]
        }
    ]
}